{
  "manifest_version": 2,
  "name": "makeRss",
  "description": "makeRss",
  "version": "0.0.2",
  "icons": {
    "16": "/icons/icon_16.png",
    "48": "/icons/icon_48.png",
    "128": "/icons/icon_128.png"
  },
  "page_action": {
    "default_title": "generate RSS item"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.gamekult.com/*"
      ],
      "js": [
        "index.ts"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.ts"
    ]
  }
}